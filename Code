local player = game.Players.LocalPlayer
local chat = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest

player.Chatted:Connect(function(message)
  if message == "Kenzie you are amazing" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("awww", "All")
  elseif message == "my life is now better, you fixed my past and will be my future" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("ğŸ˜", "All")
  elseif message == "I know we haven't been together long but I think I want to spend my life with you" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("starts freaking out", "All")
  elseif message == "I know this is a Roblox game, but you are beautiful and kind and just perfect and I.." and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("are you saying what I think you are going to say", "All")
  elseif message == "Kenzie will you marry me and be mine forever ğŸ’" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("yes of course I will, I want to be with you forever and ever", "All")
  elseif message == "gets on my knee's" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("puts out my finger", "All")
  elseif message == "Puts ring on finger ..mwa.. Ly ..and you will always belong to me" and player.Name == "bunnyswopper12" then
    wait(3)
    chat:FireServer("Mwa.. Ly to.. and yes daddy I'm all yours", "All")
  end
end)
